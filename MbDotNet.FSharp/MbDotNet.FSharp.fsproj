<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>

    <!-- NuGet Metadata-->
    <Version>0.1.0-alpha</Version>
    <PackageId>MbDotNet.FSharp</PackageId>
    <Title>MbDotNet.FSharp</Title>
    <Authors>Matthew Herman</Authors>
    <Description>An F# wrapper around MbDotNet</Description>
    <Copyright>Copyright ©  2018</Copyright>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>http://github.com/mattherman/MbDotNet.FSharp/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>http://github.com/mattherman/MbDotNet.FSharp</PackageProjectUrl>
    <PackageReleaseNotes>https://github.com/mattherman/MbDotNet.FSharp/releases</PackageReleaseNotes>
    <PackageTags>mountebank client wrapper</PackageTags>
    <BuildOutputTargetFolder>lib</BuildOutputTargetFolder>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Library.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MbDotNet" Version="4.2.0" />
  </ItemGroup>

</Project>
